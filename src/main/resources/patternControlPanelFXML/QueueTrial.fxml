<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="205.0" prefWidth="206.0" xmlns="http://javafx.com/javafx/21.0.7" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.patternControlPanel.startMenu.QueueTrialController">
   <children>
      <VBox layoutX="24.0" layoutY="23.0" prefHeight="149.0" prefWidth="158.0" spacing="10.0">
         <children>
            <Label prefHeight="27.0" prefWidth="106.0" text="Saved Trials">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label text="Choose a trial">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <ComboBox fx:id="savedTrialsComboBox" prefWidth="150.0">
               <VBox.margin>
                  <Insets top="-5.0" />
               </VBox.margin>
            </ComboBox>
            <Button fx:id="addTrialToQueue" mnemonicParsing="false" onAction="#handleAddTrialToQueueClick" text="Add to Queue">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
